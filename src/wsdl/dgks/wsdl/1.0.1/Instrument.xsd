            <xs:element name="ProductType" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Model" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="SoftwareVersion" type="xs:string" minOccurs="0" maxOccurs="1"/>

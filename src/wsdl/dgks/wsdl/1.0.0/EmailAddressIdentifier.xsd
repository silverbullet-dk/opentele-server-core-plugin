    <element name="EmailAddressIdentifier" type="mc:EmailAddressIdentifierType"/>
    
    <simpleType name="EmailAddressIdentifierType"> 
        <restriction base="string"> 
            <pattern value="[^@]+@[^\.]+\..+"/> 
        </restriction> 
    </simpleType> 
</schema>
